{
  "name": "cryptodock",
  "productName": "cryptodock",
  "version": "0.0.1",
  "description": "Trading Farm For Algo Trading With NodeJS and Python",
  "bin": {
    "dock": "./bin/index.js"
  },
  "keywords": [
    "Trading",
    "Crypto",
    "Markets",
    "NodeJS",
    "Python"
  ],
  "author": {
    "name": "Daniel Griffiths | The Launch",
    "email": "daniel@thelaunch.tech"
  },
  "license": "ISC",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "npm run compile && electron-builder",
    "dist:dir": "npm run dist --dir -c.compression=store -c.mac.identity=null",
    "build:css": "postcss src/renderer/assets/tailwind.css -o src/renderer/assets/tailwind.scss"
  },
  "electronWebpack": {
    "commonSourceDirectory": "src/common",
    "staticSourceDirectory": "src/static",
    "title": "CryptoDock",
    "whiteListedModules": [],
    "main": {
      "extraEntries": [],
      "sourceDirectory": "src/main",
      "webpackConfig": "webpack.general.config.js"
    },
    "renderer": {
      "dll": [],
      "sourceDirectory": "src/renderer",
      "webpackConfig": "webpack.general.config.js"
    }
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.7.3",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.4.0",
    "electron": "5.0.6",
    "electron-builder": "^21.0.11",
    "electron-webpack": "^2.7.4",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "tailwindcss": "^1.1.4",
    "tailwindcss-transitions": "^2.1.0",
    "webpack": "~4.35.3",
    "webpack-cli": "^3.3.10"
  }
}
